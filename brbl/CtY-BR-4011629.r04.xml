                  <marc:datafield tag="245" ind1="1" ind2="0">
                     <marc:subfield code="a">Anthony Dey Galveston Bay and Texas Land Company records,</marc:subfield>
                     <marc:subfield code="f">1821-1855</marc:subfield>
